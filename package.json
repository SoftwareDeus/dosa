{
	"name": "useit",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit build",
		"build:mobile": "svelte-kit build && npx cap sync",
		"dev:ios": "npx cap run ios",
		"dev:android": "npx cap run android",
		"sync": "npx cap sync",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"lint:check": "prettier --check src/ e2e/ && eslint src/ e2e/ --max-warnings 0",
		"lint:fix": "prettier --write src/ e2e/ && eslint src/ e2e/ --fix",
		"dev:safe": "npm run lint:check && npm run dev",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"prisma:generate": "prisma generate",
		"prisma:migrate": "prisma migrate dev",
		"prisma:deploy": "prisma migrate deploy",
		"sentry:release": "sentry-cli releases new  && sentry-cli releases set-commits --auto  && sentry-cli releases files  upload-sourcemaps .svelte-kit/output && sentry-cli releases finalize "
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.1.1",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.22.0",
		"@inlang/paraglide-js": "^2.0.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@playwright/test": "^1.49.1",
		"@sentry/cli": "^2.54.0",
		"@storybook/addon-a11y": "^9.1.7",
		"@storybook/addon-docs": "^9.1.7",
		"@storybook/addon-svelte-csf": "^5.0.8",
		"@storybook/addon-vitest": "^9.1.7",
		"@storybook/sveltekit": "^9.1.7",
		"@sveltejs/adapter-static": "^0.0.17",
		"@sveltejs/adapter-vercel": "^5.10.2",
		"@sveltejs/app-utils": "^1.0.0-next.0",
		"@sveltejs/kit": "^2.42.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^22",
		"@vitest/browser": "^3.2.3",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-sonarjs": "^3.0.5",
		"eslint-plugin-storybook": "^9.1.7",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"playwright": "^1.53.0",
		"postcss": "^8.5.6",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"prisma": "^6.16.2",
		"storybook": "^9.1.7",
		"svelte": "^5.39.3",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.1.6",
		"vite-plugin-devtools-json": "^1.0.0",
		"vite-plugin-pwa": "^1.0.3",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0",
		"workbox-window": "^7.3.0"
	},
	"dependencies": {
		"@aparajita/capacitor-secure-storage": "^7.1.3",
		"@capacitor/android": "^7.4.3",
		"@capacitor/app": "^7.1.0",
		"@capacitor/camera": "^7.0.2",
		"@capacitor/cli": "^7.4.3",
		"@capacitor/core": "^7.4.3",
		"@capacitor/device": "^7.0.2",
		"@capacitor/filesystem": "^7.1.4",
		"@capacitor/geolocation": "^7.1.5",
		"@capacitor/haptics": "^7.0.2",
		"@capacitor/ios": "^7.4.3",
		"@capacitor/keyboard": "^7.0.3",
		"@capacitor/local-notifications": "^7.0.3",
		"@capacitor/network": "^7.0.2",
		"@capacitor/preferences": "^7.0.2",
