		"@node-rs/argon2": "^2.0.2",
		"@prisma/client": "^6.16.2",
		"@sentry/capacitor": "^2.3.1",
		"@sentry/sveltekit": "9.46.0",
		"@supabase/ssr": "^0.7.0",
		"@supabase/supabase-js": "^2.57.4",
		"jose": "^6.1.0",
		"postgres": "^3.4.5",
		"zod": "^4.1.11"
	}
}
